void setup () {
  size (400,400);
  background (200);
  strokeWeight(5);
  stroke(0);
  noFill();
  rect(1, 1, 397, 397);

  fill (0);
  textSize (14);
  text ( "Esto es un cuadrado verde:", 20,100); 
  text ( "Esto es una ellipse azul:", 20, 250);
 
  strokeWeight(2);
  fill (0,255,0);
  rect (250, 50, 100,100);
  strokeWeight (2);
  fill (0,0,255);
  ellipse (300, 250, 100,100);
}
